{
  "compilerOptions": {
    "target": "ESNext",                               
    "module": "ESNext",                                
    "moduleResolution": "node",                    
    "esModuleInterop": true,  
    "jsx": "react-jsx",                          
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                      
    "skipLibCheck": true,   
    "allowJs":false,
    "noEmit": true                
  },
  "include": ["src"]
}
