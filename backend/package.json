{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend for the SPR and Level check form",
  "main": "server.js",
  "scripts": {
  "start": "NODE_ENV=production node src/server.js",
  "dev": "NODE_ENV=development nodemon src/server.js",
  "generate": "cp .env.development .env && prisma generate",
  "migrate": "cp .env.development .env && prisma migrate dev",
  "migrate:deploy": "cp .env.production .env && prisma migrate deploy",
  "seed": "node seed.js"
  },
